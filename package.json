{
  "name": "@gabortorma/mwm-nuxt-module-template",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
  "description": "My new Nuxt module",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "git+ssh://git@github.com/GaborTorma/mwm-nuxt-module-template.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && dotenv release-it",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "postinstall": "nuxt-module-build prepare && nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.2"
  },
  "devDependencies": {
    "@gabortorma/mwm": "^0.8.8",
    "@gabortorma/nuxt-eslint-layer": "^0.1.0",
    "@nuxt/devtools": "^1.5.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.2",
    "@types/node": "^22.5.5",
    "dotenv-cli": "^7.4.2",
    "nuxt": "^3.13.2",
    "release-it": "^17.6.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1",
    "vue-tsc": "^2.1.6"
  }
}
